    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.4",
    "next-themes": "^0.3.0",
    "idb": "^8.0.0",
    "uuid": "^10.0.0",
    "react-hook-form": "^7.54.0",
    "zod": "^3.24.1",
    "@hookform/resolvers": "^3.10.0",
    "@supabase/supabase-js": "^2.39.0",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
